@model List<MyMVC.Models.MemberViewModel>
@{
    ViewBag.Title = "Member";
}

<h2>Member</h2>
<button onclick="location.href='@Url.Action("Create")'">新增</button>
@{ 
    <table>
        <tr>
            <td>@Html.LabelFor(model => model.First().ID)</td>
            <td>@Html.LabelFor(model => model.First().Name)</td>
            <td>@Html.LabelFor(model => model.First().PhoneNo)</td>
        </tr>
        @foreach (var item in Model)
        {
            <tr>               
                <td>@Html.DisplayFor(model => item.ID)</td>            
                <td>@Html.DisplayFor(model => item.Name)</td>
                <td>@Html.DisplayFor(model => item.PhoneNo)</td>
            </tr>
        }        
    </table>
}

